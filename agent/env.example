# Server Configuration
PORT=3000

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# EVM Configuration
CHAIN_NAME=sepolia
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
MAINNET_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
POLYGON_RPC_URL=https://polygon-rpc.com

# WalletConnect (for dApp connections)
WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here 

# Google Calendar Configuration
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour private key here\n-----END PRIVATE KEY-----"
# Calendar ID - use 'primary' for service account's own calendar, or the shared calendar ID
GOOGLE_CALENDAR_ID=primary
# For shared calendars, use the full calendar ID (e.g., your-email@gmail.com)
# GOOGLE_CALENDAR_ID=your-email@gmail.com

# Agent Configuration
OPENSERV_API_KEY=
OPENAI_API_KEY=

# NameStone Configuration (optional - for ENS subname creation)
NAMESTONE_API_KEY=your_namestone_api_key_here